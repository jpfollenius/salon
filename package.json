{
  "name": "typescript-react-webpack",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack -p --config webpack.config.prod",
    "start": "webpack-dev-server --config webpack.config.dev --inline",
    "test": "jest --watch",
    "prebublish": "typings install"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.14.1",
    "ts-jest": "^19.0.2",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "apeman-react-clock": "^3.0.3",
    "classnames": "^2.2.5",
    "firebase": "^3.7.2",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "mobx-react-devtools": "^4.2.11",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-big-calendar": "^0.14.0",
    "react-bootstrap": "^0.30.8",
    "react-custom-scrollbars": "^4.1.2",
    "react-datepicker": "^0.47.0",
    "react-dom": "^15.4.2",
    "recharts": "^1.0.0-alpha.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$"
  }
}
